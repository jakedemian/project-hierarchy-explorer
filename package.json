{
  "name": "project-hierarchy-explorer",
  "displayName": "Project Hierarchy Explorer",
  "description": "Quickly and easily output your full project tree / hierarchy to a text file.",
  "author": {
    "name": "Jake Demian",
    "email": "jakedemian@gmail.com"
  },
  "publisher": "jake-demian",
  "version": "1.0.2",
  "icon": "images/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakedemian/project-hierarchy-explorer"
  },
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:project-hierarchy-explorer.generate"
  ],
  "contributes": {
    "commands": [
      {
        "command": "project-hierarchy-explorer.generate",
        "title": "Project Hierarchy Explorer: Generate"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Project Hierarchy Explorer",
      "properties": {
        "project-hierarchy-explorer.ignorePatterns": {
          "type": "array",
          "default": [],
          "description": "Array of patterns to ignore when generating the directory structure."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "clean": "rimraf ./out/*",
    "compile": "yarn run clean && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/dedent": "^0.7.0",
    "@types/glob": "^8.1.0",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "chai": "^4.3.7",
    "dedent": "^0.7.0",
    "eslint": "^8.43.0",
    "glob": "^8.1.0",
    "minimatch": "^5.1.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "typescript": "^5.1.3"
  }
}
